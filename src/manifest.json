{
  "manifest_version": 2,

  "name": "Zahada",
  "description": "This extension is for Zahada",
  "version": "1.0",
  "background" : {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": "images/icon.png",
    "default_popup": "html/popup.html"
  },
  "permissions": [
	"declarativeContent",
    "activeTab",
    "http://www.mcgov.co.uk/"
  ]
}
